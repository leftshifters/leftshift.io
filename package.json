{
  "name": "leftshift-website",
  "version": "1.1.9",
  "main": "app.js",
  "license": "MIT",
  "scripts": {
    "start": "node lib/app.js",
    "test": "jest --collect-coverage",
    "coverage": "open ./coverage/lcov-report/index.html",
    "lint": "eslint *.js content/**/*.js",
    "build": "docker build -t leftshifters/leftshift.io .",
    "rmi": "docker rmi leftshifters/leftshift.io",
    "run": "docker run -p 8080:8080 -d --name leftshift.io leftshifters/leftshift.io",
    "dstart": "docker start leftshift.io",
    "stop": "docker stop leftshift.io",
    "rm": "docker rm leftshift.io",
    "logs": "docker logs leftshift.io",
    "top": "docker top leftshift.io"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "errorhandler": "^1.5.1",
    "express": "4.17.1",
    "markdown": "~0.5.0",
    "marked": "^0.8.2",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "pug": "2.0.4",
    "serve-favicon": "^2.5.0",
    "underscore": "~1.10.2"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1"
  }
}
